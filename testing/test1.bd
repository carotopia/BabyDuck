program CompilerV1;

var n, factorial, i: int;

void printNumber(n : int) [
{
    print("El numero es", n + n * 10);
}
];

void a(n: int) [
   {
    printNumber(n);
   }
];

main
{
    n = 5;
    factorial = 1;

    i = n;
    while (i > 1) do {
        factorial = factorial * i;
        i = i - 1;
    };

    print("factorial", factorial);
    a(5);
}

end