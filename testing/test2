program BabyDuck;

var
    globalCounter, counter, loopIndex: int;
    totalSum, averageResult, temporaryValue: float;

void processInputs(floatInput: float, intInput: int) [
    var
        sumResult, rowIndex, columnIndex: int;
        computedFloat: float;

    {
        rowIndex = 1;
        columnIndex = 2;
        sumResult = 0;
        computedFloat = 3.0;
        globalCounter = floatInput + 1;
    }
];

void processSecondaryInputs(floatInput: float, intInput: int) [
    var
        sumResult, rowIndex, columnIndex: int;
        computedFloat: float;

    {
        rowIndex = 1;
        columnIndex = 2;
        sumResult = 0;
        computedFloat = 3.0;
        globalCounter = floatInput + 1;
        pi = 10;
    }
];

main
{
    totalSum = 1 + 2;
    globalCounter = (1 + 2) / 3;

    counter = 1;

    print(totalSum > abc);
    print(totalSum + 45);

    if (yy > 10) {
        print("counter is 1");
    } else {
        print("counter is not 1");
    };

