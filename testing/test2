program TestSimple;

var
  a, b, c, d, e, f: int;

void rana(num: int) [
  var
    temp: int;
  {
    temp = num * 2 + 2;
    print(temp);
  }
];

main {
  a = 10;
  b = 5;
  c = 3;
  d = 2;

  e = a + b * c - d;

  f = (a - b) * (c + d);

  print("Valor de e:");
  print(e);

  print("Valor de f:");
  print(f);

  if (a > b) {
    print("a es mayor que b");
  } else {
    print("a no es mayor que b");
  };

  if (c < d) {
    print("c es menor que d");
  } else {
    print("c no es menor que d");
  };

  while (a > 0) do {
    print(a);
    a = a - 1;
  };

  rana(5);
  rana(8);

  print("resultado final:");
  print(a + b + c + d + e + f);
}
end