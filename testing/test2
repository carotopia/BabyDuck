program MathTest;

var
  a, b, c, d: int;
  resultado1, resultado2, resultado3, resultado4: int;
  final: int;

main {
  a = 20;
  b = 4;
  c = 8;
  d = 2;


  resultado1 = a / b;
  print("Test 1 - División:");
  print("20 / 4 =");
  print(resultado1);


  resultado2 = a + b * c - d;
  print("Test 2 - Precedencia:");
  print("20 + 4 * 8 - 2 =");
  print(resultado2);

  resultado3 = a * d + c / b;
  print("Test 3 - Multiplicación y división:");
  print("20 * 2 + 8 / 4 =");
  print(resultado3);

  resultado4 = c / d + a / b * c;
  print("Test 4 - Divisiones exactas:");
  print("8 / 2 + 20 / 4 * 8 =");
  print(resultado4);

  final = resultado1 + resultado2 + resultado3 + resultado4;
  print("Suma de todos los resultados:");
  print(final);

  print("=== RESULTADOS ESPERADOS ===");
  print("Test 1: 5");
  print("Test 2: 50");
  print("Test 3: 42");
  print("Test 4: 44");
  print("Final: 141");
}
end